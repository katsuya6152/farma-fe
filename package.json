    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vercel": "^34.2.6",
    "wrangler": "^3.60.0"
  },
  "volta": {
    "node": "20.14.0",
    "npm": "10.8.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
