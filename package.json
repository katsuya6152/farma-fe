    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-table": "^8.17.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.395.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.3.0",
